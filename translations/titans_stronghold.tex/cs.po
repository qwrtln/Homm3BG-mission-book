#. type: multicols*
#: coops/titans_stronghold.tex:53
#, fuzzy, no-wrap
#| msgid ""
#| "\\textbf{For a 2-player Scenario:}\n"
#| "\\begin{itemize}\n"
#| "  \\item 2 × Starting (I) Map Tile\n"
#| "  \\item 2 × Near (IV--V) Map Tile\n"
#| "  \\item 4 × Far (II--III) Map Tile\n"
#| "  \\item 1 × Center (VI--VII) Map Tile, which must contain the Dragon Utopia Field\n"
#| "\\end{itemize}\n"
#| "\n"
msgid ""
"\\begin{itemize}\n"
"  \\item P × Starting (I) Map Tile\n"
"  \\item 2P × Far (II--III) Map Tile\n"
"  \\item 2P × Near (IV--V) Map Tile\n"
"  \\item (P + 1) × Center (VI--VII) Map Tile\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\textbf{Scénář pro 2 hráče:}\n"
"\\begin{itemize}\n"
"  \\item 2 × Startovní (I) Dílek Mapy\n"
"  \\item 2 × Blízký (IV--V) Dílek Mapy\n"
"  \\item 4 × Vzdálený (II--III) Dílek Mapy\n"
"  \\item 1 × Středový (VI--VII) Dílek Mapy, který musí obsahovat Pole Dračí Utopie\n"
"\\end{itemize}\n"
"\n"

#. type: multicols*
#: coops/titans_stronghold.tex:57
#, fuzzy, no-wrap
#| msgid ""
#| "If you don't have enough VI--VII Tiles, you can use another Tile near Tile I.\n"
#| "The center of this Tile is the Titans' Stronghold.\n"
#| "\n"
msgid ""
"\\textbf{\\MakeUppercase{Note:}} If you don’t have enough VI--VII Tiles, you can\n"
"use another Tile near I Tiles. The center of this\n"
"Tile is the Titans’ Stronghold.\n"
"\n"
msgstr ""
"Pokud nemáte dostatek Dílků VI--VII, můžete použít jiný Dílek blízko Dílku I.\n"
"Střed tohoto Dílku je Pevnost Titánů.\n"
"\n"

#. type: multicols*
