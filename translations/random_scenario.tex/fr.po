#. type: multicols
#: sections/random_scenario.tex:302
#, no-wrap
msgid ""
"\\textbf{\\nth{1} Round:}\n"
"\\begin{itemize}\n"
"  \\item All Heroes gain +1 \\svgeven{movement}.\n"
"\\end{itemize}\n"
"\\textbf{\\nth{6} and \\nth{9} Rounds:}\n"
"\\begin{itemize}\n"
"  \\item Remove all Black Cubes from every Water Wheel and Windmill on the map.\n"
"\\end{itemize}\n"
"\\textbf{\\nth{10} Round:}\n"
"\\begin{itemize}\n"
"  \\item Player(s) whose Main Hero has the least experience roll 2 \\svg{resource}.\n"
"\\end{itemize}\n"
"\\textbf{\\nth{12} Round:}\n"
"\\begin{itemize}\n"
"  \\item Repeat event of Round 6.\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\textbf{Manche 1:}\n"
"\\begin{itemize}\n"
"  \\item Les Héros gagnent +1 \\svgeven{movement}.\n"
"\\end{itemize}\n"
"\\textbf{Manches 6 et 9:}\n"
"\\begin{itemize}\n"
"  \\item Retirez les Cubes Noirs de chaque Moulin à Eau et Moulin à Vent sur le Plateau.\n"
"\\end{itemize}\n"
"\\textbf{Manche 10:}\n"
"\\begin{itemize}\n"
"  \\item Le Joueur dont le Héros Principal a le moins d'expérience lances 2 \\svg{resource}.\n"
"\\end{itemize}\n"
"\\textbf{Manche 12:}\n"
"\\begin{itemize}\n"
"  \\item Repetez les évènements de la Manche 6.\n"
"\\end{itemize}\n"
"\n"

#. type: multicols
#: sections/random_scenario.tex:312
#, no-wrap
msgid ""
"\\begin{itemize}\n"
"  \\item When Visiting a an Obelisk, roll 2 \\svg{treasure} and choose 1 to resolve.\n"
"  \\item Players can spend their Build Token at any time to perform Trading Post actions.\n"
"  \\item Level VII Combat cannot be skipped by Diplomacy or any other means.\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\begin{itemize}\n"
"  \\item En Visitant un Obélisque, lancez 2 \\svg{treasure} et choisir un résultat.\n"
"  \\item Les Joueurs peuvent utiliser leurs Jetons Construction librement pour faire les actions du Comptoir.\n"
"  \\item Les Combats Niveau VII ne peuvent pas être évités par Diplomatie ou d'autres moyens.\n"
"\\end{itemize}\n"
"\n"
