#. type: multicols*
#: clash/force_of_will.tex:46
#, fuzzy, no-wrap
#| msgid ""
#| "\\textbf{For a 2-player Scenario:}\n"
#| "\\begin{itemize}\n"
#| "  \\item 2 × Starting (I) Map Tile\n"
#| "  \\item 4 × Near (IV--V) Map Tile, all of which must contain an Obelisk\n"
#| "  \\item 4 × Far (II--III) Map Tile, 2 of which must contain a Settlement\n"
#| "  \\item 1 × Center (VI--VII) Map Tile, which must contain the Grail Field\n"
#| "\\end{itemize}\n"
#| "\n"
msgid ""
"\\textbf{For a 2-player scenario:}\n"
"\\begin{itemize}\n"
"  \\item 2 × Starting (I) Map Tile\n"
"  \\item 2 × Near (IV--V) Map Tile\n"
"  \\item 4 × Far (II--III) Map Tile\n"
"  \\item 1 × Center (VI--VII) Map Tile, which must contain the Dragon Utopia Field\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\textbf{Scénář pro 2 hráče:}\n"
"\\begin{itemize}\n"
"  \\item 2 × Startovní (I) Dílek Mapy\n"
"  \\item 4 × Blízký (IV--V) Dílek Mapy, všechny musí obsahovat Obelisk\n"
"  \\item 4 × Vzdálený (II--III) Dílek Mapy, z nichž 2 musí obsahovat Osadu\n"
"  \\item 1 × Středový (VI--VII) Dílek Mapy, který musí obsahovat Pole s Grálem\n"
"\\end{itemize}\n"
"\n"

#. type: multicols*
#: clash/force_of_will.tex:54
#, fuzzy, no-wrap
#| msgid ""
#| "\\textbf{For a 4-player Scenario:}\n"
#| "\\begin{itemize}\n"
#| "  \\item 4 × Starting (I) Map Tile\n"
#| "  \\item 6 × Near (IV--V) Map Tile, 4 of which must contain an Obelisk\n"
#| "  \\item 8 × Far (II--III) Map Tile, 4 of which must contain a Settlement\n"
#| "  \\item 1 × Center (VI--VII) Map Tile, which must contain the Grail Field\n"
#| "\\end{itemize}\n"
#| "\n"
msgid ""
"\\textbf{For a 4-player scenario:}\n"
"\\begin{itemize}\n"
"  \\item 3 × Starting (I) Map Tile\n"
"  \\item 3 × Near (IV--V) Map Tile\n"
"  \\item 6 × Far (II--III) Map Tile\n"
"  \\item 1 × Center (VI--VII) Map Tile, which must contain the Dragon Utopia Field\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\textbf{Scénář pro 4 hráče:}\n"
"\\begin{itemize}\n"
"  \\item 4 × Startovní (I) Dílek Mapy\n"
"  \\item 6 × Blízký (IV--V) Dílek Mapy, z nichž 4 musí obsahovat Obelisk\n"
"  \\item 8 × Vzdálený (II--III) Dílek Mapy, z nichž 4 musí obsahovat Osadu\n"
"  \\item 1 × Středový (VI--VII) Dílek Mapy, který musí obsahovat Pole s Grálem\n"
"\\end{itemize}\n"
"\n"

#. type: multicols*
#: clash/force_of_will.tex:57
#, fuzzy, no-wrap
#| msgid ""
#| "\\subsection*{\\MakeUppercase{Victory Conditions}}\n"
#| "To win the Scenario, a Hero must obtain the Grail Token and bring it to their Faction Town.\n"
#| "\n"
msgid ""
"\\subsection*{\\MakeUppercase{Victory Conditions}}\n"
"To win the Scenario, a Hero must Flag the Dragon Utopia Field.\n"
"\n"
msgstr ""
"\\subsection*{\\MakeUppercase{Podmínky Vítězství}}\n"
"Aby Hrdina vyhrál Scénář, musí získat Žeton Grálu a přinést jej do Města své Frakce.\n"
"\n"

#. type: multicols*
#: clash/force_of_will.tex:60
#, fuzzy, no-wrap
#| msgid ""
#| "\\subsection*{\\MakeUppercase{Defeat Conditions}}\n"
#| "If the Dragon Boss still lives at the end of the \\nth{14} Round, all players lose the Scenario.\n"
#| "\n"
msgid ""
"\\subsection*{\\MakeUppercase{Defeat Conditions}}\n"
"At the end of the \\nth{12} Round, if there is no winner, all players lose the Scenario.\n"
"\n"
msgstr ""
"\\subsection*{\\MakeUppercase{Podmínky Porážky}}\n"
"Pokud Dračí Boss stále žije na konci 14. Kola, všichni hráči prohrávají Scénář.\n"
"\n"

#. type: multicols*
#: clash/force_of_will.tex:67
#, fuzzy, no-wrap
#| msgid ""
#| "\\textbf{\\nth{4}, \\nth{8} and \\nth{12} Rounds:}\n"
#| "\\begin{itemize}\n"
#| "  \\item Remove all Black cubes from every Windmill, Water Wheel, and Mystical Garden on the map.\n"
#| "\\end{itemize}\n"
#| "\n"
msgid ""
"\\textbf{\\nth{4} and \\nth{8} Rounds:}\n"
"\\begin{itemize}\n"
"  \\item Remove all Black cubes from every Water Wheel and Windmill on the map.\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\textbf{4., 8. a 12. Kolo:}\n"
"\\begin{itemize}\n"
"  \\item Odstraňte všechny Černé Kostičky ze všech Větrných Mlýnů, Vodních Kol a Mystických Zahrad na mapě.\n"
"\\end{itemize}\n"
"\n"

#. type: multicols*
#: clash/force_of_will.tex:72
#, fuzzy, no-wrap
#| msgid ""
#| "\\textbf{\\nth{7} and \\nth{8} Rounds:}\n"
#| "\\begin{itemize}\n"
#| "  \\item Spawn an enemy army following the rules outlined below.\n"
#| "\\end{itemize}\n"
#| "\n"
msgid ""
"\\textbf{\\nth{5} and \\nth{9} Rounds (2-player Scenario only):}\n"
"\\begin{itemize}\n"
"  \\item The second player gains 1 \\svg{movement}.\n"
"\\end{itemize}\n"
"\n"
msgstr ""
"\\textbf{7. a 8. Kolo:}\n"
"\\begin{itemize}\n"
"  \\item Vyvolejte nepřátelskou armádu podle níže uvedených pravidel.\n"
"\\end{itemize}\n"
"\n"

#. type: multicols*
#: clash/force_of_will.tex:93
#, no-wrap
msgid ""
"\\begin{itemize}\n"
"  \\item When a player Visits an Obelisk, they roll 2 \\svg{treasure} and choose one to resolve.\n"
"  \\item A player may not use the Diplomacy card to skip Combat on the Dragon Utopia Field.\n"
"  \\item When a Hero moves to the Dragon Utopia Field, they must fight, depending on chosen difficulty, the following number of Neutral Units:\n"
"  \\begin{itemize}\n"
"    \\item \\textbf{Easy:} 1 × \\svgunit{azure}, 2 × \\svgunit{silver}, 1 × \\svgunit{bronze}\n"
"    \\item \\textbf{Normal:} 1 × \\svgunit{azure}, 1 × \\svgunit{golden}, 2 × \\svgunit{silver}\n"
"    \\item \\textbf{Hard:} 1 × \\svgunit{azure}, 3 × \\svgunit{golden}\n"
"    \\item \\textbf{Impossible:}  2 × \\svgunit{azure}, 2 × \\svgunit{golden}\n"
"  \\end{itemize}\n"
"  \\item Follow these steps when Combat begins against Neutral Units at the Dragon Utopia:\n"
"  \\begin{itemize}\n"
"    \\item Place up to 4 of your unit cards freely onto the center row of the Combat Board.\n"
"    \\item The enemy player controlling the Neutral Units places each Neutral unit in each of the 4 corners of the Combat Board.\n"
"  \\end{itemize}\n"
"\\end{itemize}\n"
"\n"
msgstr ""

